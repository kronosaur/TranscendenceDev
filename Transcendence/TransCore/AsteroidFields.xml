<TranscendenceModule>

<!-- ASTEROID FIELD MORPHOLOGIES

	These definitions place asteroids and labels in various configurations
	(morphologies). The actual type of asteroid (or label) created is passed
	in as a Lookup table parameter.

	The following parameters are generally used:

	_AsteroidSize{X}: This should be an asteroid of size class X.

	_Label: This should be a label definition for the locations inside the 
			field. Generally, the label should only include attributes related
			to the type of asteroid field (metallic, rocky, etc.).

-->

	<SystemPartTable unid="&tbAsteroidFieldMorphologies;">

		<!-- NEW SCALE =========================================================
		
			In API 50 we're slowly introducing a new image scale for asteroids
			and planets. All worlds look bigger in the new scale, which means
			we don't need to create as many large asteroids in fields.

			To distinguish from the old scale, we call these "patterns" instead
			of "morphologies".

			In general, asteroids fields should have the following proportions
			of sizes:

			Size A		95.0 %
			Size B       4.0 %
			Size C       0.8 %
			Size D       0.2 %

		-->

		<AsteroidSingletonPattern>
			<Table>
				<Lookup chance="950"	table="_AsteroidSizeA"/>
				<Lookup chance="40"		table="_AsteroidSizeB"/>
				<Lookup chance="8"		table="_AsteroidSizeC"/>
				<Lookup chance="2"		table="_AsteroidSizeD"/>
			</Table>
		</AsteroidSingletonPattern>

		<AsteroidSingletonWithLocationPattern>

			<!-- NOTE: We end up with roughly 1.4 locations per 100 asteroids -->

			<Table>
				<Lookup chance="950"	table="_AsteroidSizeA"/>
				<Group  chance="40">
					<Lookup	table="_AsteroidSizeB"/>

					<Orbitals chance="20" distance="2d4+4" angle="random">
						<LocationAttributes attributes="asteroids, asteroidBelt">
							<Lookup table="_Location"/>
						</LocationAttributes>
					</Orbitals>
				</Group>
				<Group  chance="8">
					<Lookup	table="_AsteroidSizeC"/>

					<Orbitals chance="50" distance="2d4+6" angle="random">
						<LocationAttributes attributes="asteroids, asteroidBelt">
							<Lookup table="_Location"/>
						</LocationAttributes>
					</Orbitals>
				</Group>
				<Group  chance="2">
					<Lookup	table="_AsteroidSizeD"/>

					<Orbitals distance="2d4+8" angle="random">
						<LocationAttributes attributes="asteroids, asteroidBelt">
							<Lookup table="_Location"/>
						</LocationAttributes>
					</Orbitals>
				</Group>
			</Table>
		</AsteroidSingletonWithLocationPattern>

